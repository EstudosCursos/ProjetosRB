
public class AutenticacaoCache {
	private int senha;
	public AutenticacaoCache() {
		// TODO Auto-generated constructor stub
	}
	@Override
	public boolean autentica(int senha) {
		if(this.senha == senha){
			return true;
		}else{
			return false;
		}	
	}
	@Override
	public void setSenha(int senha) {
		this.senha = senha;
		}

}
