
public class Gerente implements Autenticavel {
	private int senha;
	public double getBonificacao(){
		System.out.println("Gerente");
			return 300;
		}
	@Override
	public boolean autentica(int senha) {
		if(this.senha == senha){
			return true;
		}else{
			return false;
		}
		
	}

	@Override
	public void setSenha(int senha) {
		this.senha = senha;
		}
	
}