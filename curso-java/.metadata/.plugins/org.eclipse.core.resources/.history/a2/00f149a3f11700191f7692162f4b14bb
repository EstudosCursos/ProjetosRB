
public class Teste {
	
	public static void main(String[] args) {
		Funcionario teste = new Funcionario();
		
		teste.setSalario(3000.0);
		System.out.println(teste.getTipo());
		System.out.println(teste.getBonificacao());
		
		Funcionario teste1 = new Funcionario();
		teste1.setSalario(3000.0);
		teste1.setTipo(1);
		System.out.println(teste1.getTipo());
		System.out.println(teste1.getBonificacao());
		
		Funcionario teste2 = new Funcionario();
		teste2.setSalario(3000.0);
		teste2.setTipo(2);
		System.out.println(teste2.getTipo());
		System.out.println(teste2.getBonificacao());
		
	}


}
